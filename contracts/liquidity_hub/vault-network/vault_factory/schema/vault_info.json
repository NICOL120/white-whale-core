{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "VaultInfo",
  "description": "Response for the vaults query",
  "type": "object",
  "required": [
    "asset_info_reference",
    "vault"
  ],
  "properties": {
    "asset_info_reference": {
      "type": "array",
      "items": {
        "type": "integer",
        "format": "uint8",
        "minimum": 0.0
      }
    },
    "vault": {
      "type": "string"
    }
  }
}
